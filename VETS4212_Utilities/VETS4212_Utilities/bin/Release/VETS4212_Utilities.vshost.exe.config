<?xml version="1.0"?>
<configuration>
  <!-- Specific Application Setting -->
  <!--
    Keys logEnabled and internalAccess are defaulted to false
    these items should not be included to external users
  -->
  <appSettings>
    <add key="webAddress" value="https://vets100.dol.gov/vets100/" />
    <add key="searchAddress" value="https://vets100.dol.gov/vets100/External/Report/ExecuteSearch" />
    <add key="pdfWebAddress" value="https://vets100.dol.gov/vets100/External/Report/Pdf/" />
    <add key="companyInformationAddress" value="https://vets100.dol.gov/vets100/External/Company/View/" />
    <add key="dolApiAddress" value="https://api.dol.gov/v1/vets100/" />
    <add key="logEnabled" value="true" />
    <add key="internalAccess" value="false" />
    <add key="bypassCompanyValidation" value="true" />
  </appSettings>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/>
  </startup>
</configuration>
